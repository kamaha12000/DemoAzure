{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "contactfromlocaltoazure"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStore2')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "https://inodalake.azuredatalakestore.net/webhdfs/v1",
					"tenant": "0240d3b8-e873-4a8f-acf3-90f41a537f35",
					"subscriptionId": "b4b4258f-d8aa-4606-8e05-8e0827fd0d27",
					"resourceGroupName": "datafactory"
				}
			}
		}
	]
}